<%= form_for @changeset, @action, fn f -> %>
  <div class="form-group">
    <%= label f, :contact, class: "control-label" %>
    <%= select f, :contact_id, @contacts %>
    <%= error_tag f, :contact %>
    <%= link "update", to: "/freeagent/sync_contacts" %>
  </div>

  <div class="form-group">
    <%= label f, :Service, class: "control-label" %>
    <%= select f, :price_list_item_id, @price_list_items %>
    <%= error_tag f, :price_list_item %>
    <%= link "update", to: "/freeagent/sync_price_list_items" %>
  </div>

  <div class="form-group">
    <%= label f, :Quantity_Per_Day, class: "control-label" %>
    <%= select f, :quantity, @quantity_options %>
    <%= error_tag f, :quantity %>
  </div>

  <div class="form-group">
    <%= label f, :start_date, class: "control-label" %>
    <%= date_input f, :start_date %>
    <%= error_tag f, :start_date %>
  </div>

  <div class="form-group">
    <%= label f, :end_date, class: "control-label" %>
    <%= date_input f, :end_date %>
    <%= error_tag f, :end_date %>
  </div>

  <div class="form-group">
    <%= label f, :description, class: "control-label" %>
    <%= text_input f, :description, class: "form-control" %>
    <%= error_tag f, :description %>
  </div>

  <div class="form-group">
    <%= submit "Submit"%>
  </div>
<% end %>
